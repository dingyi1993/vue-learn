{
  "env": {
    "browser": true
  },
  "extends": "airbnb-base",
  "rules": {
    "semi": ["error", "never"],
    "no-new": ["off"],
    "no-unused-vars": ["warn"],
    "prefer-template": ["off"],
    "no-param-reassign": ["off"],
    "no-plusplus": ["off"],
    "arrow-parens": ["off"],
    "arrow-body-style": ["off"],
    "linebreak-style": ["off"]
  }
}